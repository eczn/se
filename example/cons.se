;; 定义数据结构 pair 
(define cons
    (lambda (x y)
        (lambda (m)
            (m x y))))

;；取出 pair 的左边
(define car
    (lambda (pair) 
        (pair (lambda (l r) l))))

;; 取出 pair 的右边
(define cdr
    (lambda (pair)
        (pair (lambda (l r) r))))

;; 定义 xy 
(define xy1 (cons 1 2))
(define xy2 (cons 3 4))

;; (1 . 2)

(car xy1)
;; => 1
(cdr xy1)
;; => 2
(car xy2)
;; => 3
(cdr xy2)
;; => 4

